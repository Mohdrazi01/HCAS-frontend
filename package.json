{
  "name": "HCAS",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "api-gen": "cd ./projects/apsystem-api-client && ng-openapi-gen && cd .. && cd ..",
    "api-gen-dev": "cd ./projects/apsystem-api-client && ng-openapi-gen --config ng-openapi-gen.dev.json && cd .. && cd ..",
    "api-gen-qa": "cd ./projects/apsystem-api-client && ng-openapi-gen --config ng-openapi-gen.qa.json && cd .. && cd ..",
    "api-gen-prod": "cd ./projects/apsystem-api-client && ng-openapi-gen --config ng-openapi-gen.prod.json && cd .. && cd ..",
    "build-library": "npm run api-gen && ng build apsystem-api-client",
    "build-library-skip-gen": "ng build apsystem-api-client",
    "build-dev-library": "npm run api-gen-dev && ng build apsystem-api-client",
    "build-qa-library": "npm run api-gen-qa && ng build apsystem-api-client",
    "build-prod-library": "npm run api-gen-prod && ng build apsystem-api-client",
    "serve": "ng serve --port 4300",
    "build-local-app": "ng build",
    "build-dev-app": "ng build --configuration=dev",
    "build-qa-app": "ng build --configuration=qa",
    "build-prod-app": "ng build --configuration=production",
    "serve-local-app": "npm run serve",
    "serve-dev-app": "npm run serve --configuration=dev",
    "serve-qa-app": "npm run serve --configuration=qa",
    "serve-prod-app": "npm run serve --configuration=production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.1",
    "@angular/cdk": "^12.0.1",
    "@angular/common": "^12.0.1",
    "@angular/compiler": "^12.0.1",
    "@angular/core": "^12.0.1",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "^12.0.1",
    "@angular/localize": "^12.0.1",
    "@angular/material": "^12.0.1",
    "@angular/material-moment-adapter": "^12.0.1",
    "@angular/platform-browser": "^12.0.1",
    "@angular/platform-browser-dynamic": "^12.0.1",
    "@angular/router": "^12.0.1",
    "@kolkov/angular-editor": "^1.1.3",
    "@microsoft/applicationinsights-web": "^2.6.2",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "@rxweb/reactive-form-validators": "^2.1.3",
    "@techiediaries/ngx-textarea-autosize": "^1.3.0",
    "alertifyjs": "^1.13.1",
    "angular-inline-editable-table": "^2.0.0",
    "bootstrap": "^4.5.2",
    "chart.js": "^2.9.4",
    "exceljs": "^4.2.1",
    "file-saver": "^2.0.2",
    "font-awesome": "^4.7.0",
    "jquery": "^3.6.0",
    "jwt-decode": "^2.2.0",
    "macaddress": "^0.5.2",
    "ng-pick-datetime": "^7.0.0",
    "ng2-charts": "^2.4.2",
    "ng2-file-upload": "^1.4.0",
    "ngx-device-detector": "^2.0.10",
    "ngx-mat-select-search": "^3.3.0",
    "ngx-ui-loader": "^9.1.1",
    "run": "^1.4.0",
    "rxjs": "~6.6.0",
    "rxjs-compat": "^6.6.7",
    "sass": "^1.34.0",
    "sweetalert2": "^9.17.1",
    "tslib": "^2.2.0",
    "xlsx": "^0.16.8",
    "zone.js": "~0.11.4"
