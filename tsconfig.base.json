{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",

    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "typeRoots": [
      "node_modules/@types",
      "src/alertifytype.d.ts"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "exceljs": [
        "node_modules/exceljs/dist/exceljs.min"
      ],
      "apsystem-api-client": [
        "dist/apsystem-api-client"
      ],
      "apsystem-api-client/*": [
        "dist/apsystem-api-client/*"
      ],
      "@api": [
        "dist/apsystem-api-client"
      ],
      "@api/*": [
        "dist/apsystem-api-client/*"
      ],
      "@environments": [
        "projects/apsystem-ui/src/environments"
      ],
      "@environments/*": [
        "projects/apsystem-ui/src/environments/*"
      ],
      "@components/*": [
        "projects/apsystem-ui/src/shared/components/*"
      ],
      "@shared": [
        "projects/apsystem-ui/src/app/shared"
      ],
      "@shared/*": [
        "projects/apsystem-ui/src/app/shared/*"
      ],
      "@core": [
        "projects/apsystem-ui/src/app/core"
      ],
      "@core/*": [
        "projects/apsystem-ui/src/app/core/*"
      ],
      "@dtos/*": [
        "projects/apsystem-ui/src/app/core/dtos/*"
      ],
      "@models/*": [
        "projects/apsystem-ui/src/app/core/models/*"
      ],
      "@services/*": [
        "projects/apsystem-ui/src/app/core/services/*"
      ],
      "@services": [
        "projects/apsystem-ui/src/app/core/services"
      ],
      "@pipe/*": [
        "projects/apsystem-ui/src/app/core/pipe/*"
      ],
    }
  }
}
